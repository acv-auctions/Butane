{
  "name": "butane",
  "version": "1.0.1",
  "description": "A client for working with Firebase",
  "main": "dist/main/index.js",
  "type": "commonjs",
  "scripts": {
    "build-electron-renderer-prod": "webpack --config webpack.renderer.prod.js",
    "build-electron-main-prod": "webpack --config webpack.main.prod.js",
    "build-electron-main-dev": "webpack --config webpack.main.dev.js",
    "start-local-server": "webpack-dev-server --config webpack.renderer.dev.js",
    "start-electron": "electron dist/main/index.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jsoneditor": "^9.9.0",
    "@vue/compiler-sfc": "^3.2.37",
    "autoprefixer": "^10.4.8",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-typescript-vue3": "^2.0.17",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "electron": "^20.0.2",
    "electron-builder": "^23.3.3",
    "electron-rebuild": "^3.2.9",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "node-sass": "^7.0.1",
    "postcss-loader": "^7.0.1",
    "sass-loader": "^13.0.2",
    "tailwindcss": "^3.1.8",
    "terser-webpack-plugin": "^5.3.4",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@google-cloud/firestore": "^6.0.0",
    "@thi.ng/color": "^5.1.11",
    "cytoscape": "^3.22.1",
    "date-fns": "^2.29.1",
    "electron-settings": "^4.0.2",
    "firebase": "^9.9.2",
    "firebase-admin": "^11.9.0",
    "json-editor-vue": "^0.6.6",
    "json-formatter-js": "^2.3.4",
    "jsoneditor": "^9.9.2",
    "objects-to-csv": "^1.3.6",
    "shortid-36": "^0.1.2",
    "spawn-rx": "^3.0.0",
    "typewriter-effect": "^2.19.0",
    "v-jsoneditor": "^1.4.5",
    "vanilla-jsoneditor": "^0.7.9",
    "vue": "^3.2.37",
    "vue-shell": "^1.0.3",
    "vue3-popper": "^1.5.0",
    "vue3-shell": "^2.0.4"
  },
  "build": {
    "productName": "FireClient",
    "appId": "trueframe.fireclient",
    "buildDependenciesFromSource": false,
    "nodeGypRebuild": false,
    "npmRebuild": false,
    "win": {
      "target": [
        "portable"
      ]
    },
    "portable": {
      "artifactName": "fireclient_portable.exe"
    },
    "files": [
      "dist/renderer/**/*",
      "dist/main/index.js"
    ],
    "directories": {
      "output": "dist/electron"
    }
  }
}
